{
    "name": "takeawaytown/laravel-uuid",
    "description": "A package for using 'Universally Unique Identifiers' in Laravel.",
    "keywords": ["laravel", "uuid", "generator", "validator"],
    "type": "library",
    "require": {
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*",
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*|~6.0",
        "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*",
        "squizlabs/php_codesniffer": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1",
        "phpunit/php-code-coverage": "^5.2@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Scott",
            "email": "codetheorist@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TakeawayTown\\LaravelUuid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TakeawayTown\\LaravelUuid\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TakeawayTown\\LaravelUuid\\Providers\\UuidServiceProvider"
            ],
            "aliases": {
                "Uuid": "TakeawayTown\\LaravelUuid\\Classes\\Uuid"
            }
        }
    },
    "minimum-stability": "dev"
}
